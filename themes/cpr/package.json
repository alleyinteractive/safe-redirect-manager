{
  "name": "cpr",
  "description": "CPR Front End Build System",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx check-node-version@3.3.0 --package",
    "dev": "webpack-dev-server --config client/config/webpack.config.js --mode development --no-inline",
    "build": "webpack --config client/config/webpack.config.js --mode production",
    "scaffold": "scaffold dir ./scaffold --theme=./",
    "watch-scaffold": "nodemon -l -e json -w scaffold -x 'npm run scaffold'",
    "lint-configs": "eslint client/bin/*.js client/config/*.js",
    "docs": "doctoc ."
  },
  "engines": {
    "node": "8",
    "npm": "6"
  },
  "dependencies": {
    "js-component-framework": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "7.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "check-node-version": "^3.2.0",
    "clean-webpack-plugin": "0.1.13",
    "css-loader": "0.28.1",
    "doctoc": "^1.4.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "^1.1.11",
    "lost": "^8.3.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-libs-browser": "1.0.0",
    "node-sass": "^4.9.3",
    "postcss": "5.1.0",
    "postcss-loader": "2.0.5",
    "sass-loader": "6.0.6",
    "style-loader": "0.13.1",
    "stylelint": "8.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-livereload-plugin": "^1.2.0",
    "webpack-stats-plugin": "0.1.3"
  },
  "browserslist": [
    "last 3 versions"
  ]
}
