{
  "name": "cpr",
  "description": "CPR Front End Build System",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx check-node-version --package",
    "dev": "webpack-dev-server --config client/config/webpack.dev.config.js --env.dev",
    "watch": "webpack --config client/config/webpack.watch.config.js --debug --watch --colors --output-pathinfo --env.watch",
    "build": "webpack --config client/config/webpack.build.config.js --optimize-minimize --env.production && npm run build-admin",
    "build-admin": "webpack --config client/config/webpack.admin.config.js --optimize-minimize --env.production",
    "scaffold": "scaffold dir ./scaffold --theme=./",
    "watch-scaffold": "nodemon -l -e json -w scaffold -x 'npm run scaffold'",
    "lint-configs": "eslint client/bin/*.js client/config/*.js"
  },
  "engines": {
    "node": "8",
    "npm": "6"
  },
  "dependencies": {
    "js-component-framework": "^1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "7.0.1",
    "babel-core": "6.24.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.4.0",
    "chalk": "^2.3.0",
    "check-node-version": "^3.2.0",
    "clean-webpack-plugin": "0.1.13",
    "css-loader": "0.28.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.9.0",
    "lost": "^8.2.1",
    "node-libs-browser": "1.0.0",
    "node-sass": "4.5.2",
    "postcss": "5.1.0",
    "postcss-loader": "2.0.5",
    "sass-loader": "6.0.6",
    "style-loader": "0.13.1",
    "stylelint": "8.2.0",
    "stylelint-webpack-plugin": "0.9.0",
    "url-loader": "0.5.5",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.3",
    "webpack-livereload-plugin": "^1.0.0",
    "webpack-stats-plugin": "0.1.3"
  },
  "browserslist": [
    "last 3 versions",
    "IE 11"
  ]
}
